{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["data"],
  "properties": {
    "data": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["metadata", "data"],
        "properties": {
          "metadata": {
            "type": "object",
            "required": ["numberNodes", "timeToNextMission"],
            "properties": {
              "numberNodes": { "type": "number" },
              "timeToNextMission": { "type": "number" }
            },
            "additionalProperties": true
          },
          "data": {
            "type": "object",
            "required": ["mean"],
            "properties": {
              "count": { "type": "number" },
              "mean": { "type": "number" },
              "std": { "type": "number" },
              "ci95": {
                "type": "array",
                "minItems": 2,
                "maxItems": 2,
                "items": { "type": "number" }
              }
            },
            "additionalProperties": true
          }
        },
        "additionalProperties": false
      }
    },
    "metadata": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
